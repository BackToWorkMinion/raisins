@model IEnumerable<Raisins.Client.Web.Models.Ticket>

@{
    ViewBag.Title = "Show";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Show</h2>

<p>
    @Html.ActionLink("Back to Payments", "Index", "Payment");
</p>
<table>
    <tr>
        <th>
            TicketCode
        </th>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TicketCode)
        </td>
        <td>
            [@Html.DisplayFor(modelItem => item.Name)]
        </td>
    </tr>
}

</table>
